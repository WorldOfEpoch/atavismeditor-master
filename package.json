{
  "name": "atavism",
  "description": "Atavism Editor",
  "version": "10.11.0p5",
  "homepage": "https://atavismonline.com/",
  "main": "app/main.js",
  "author": {
    "name": "Dragonsan Studios",
    "email": "support@atavismonline.com"
  },
  "repository": {
    "type": "git"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --quiet --fix",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "lint:fix": "yarn lint --fix",
    "start": "npm-run-all -p electron:serve ng:serve",
    "start:folder": "npm-run-all -p electron:serve:folder ng:serve",
    "ng:serve": "ng serve -c dev",
    "prebuild": "node build/prebuild",
    "postbuild": "node build/postbuild",
    "build": "yarn electron:serve-tsc && ng build --base-href ./ && yarn copy:worker",
    "build:dev": "yarn electron:serve-tsc && ng build --base-href ./ -c dev && yarn copy:worker",
    "build:prod": "yarn electron:serve-tsc && ng build --base-href ./ -c production && yarn copy:worker",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && yarn electron:serve-tsc && npx electron . --serve",
    "electron:serve:test": "yarn electron:serve-tsc && npx electron . --serve",
    "electron:serve:folder": "wait-on tcp:4200 && yarn electron:serve-tsc && npx electron . --serve projectpath=/home/vasko/Documents/atavism/atavism_editor_1",
    "electron:local": "yarn electron:serve-tsc && npx electron .",
    "electron:build": "yarn build:prod && electron-builder build --publish=never",
    "mac:build": "yarn prebuild && yarn electron:build -- --mac && yarn postbuild",
    "linux:build": "yarn prebuild && yarn electron:build -- --linux && yarn postbuild",
    "win:build": "yarn prebuild && yarn electron:build -- --win && yarn postbuild",
    "copy:worker": "cd ./dist && mkdir worker && cd .. && cp ./app/worker/index.html ./dist/worker/index.html && cp ./app/worker/files-checker.js ./dist/worker/files-checker.js && yarn copy:editor-version",
    "copy:editor-version": "cp ./editor-version.txt ./dist/editor-version.txt",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "prepare": "husky install"
  },
  "private": true,
